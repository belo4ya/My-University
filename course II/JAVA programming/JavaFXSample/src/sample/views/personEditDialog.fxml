<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" styleClass="background" stylesheets="@../styles/DarkTheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.PersonEditingController">
